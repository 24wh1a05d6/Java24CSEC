package java_worksheet;

public class Pattern_b {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j,k;
		for( i=5;i>0;i--) {
			for(j=i;j>0;j--) {
				System.out.print(" ");
			}
			//System.out.println(j);
			for( k=0;k<5-(i-1);k++) {
			System.out.print("*");}
			System.out.println();
		}

	}

}
